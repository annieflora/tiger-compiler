let
  type Circle = {
    x: int,
    y: int,
    color: string
  }
  function fib(n: int): int =
    let
      var a: int := 0
      var b: int := 1
      var t := 0  /* Tiger does not have parallel assignment */
    in
      while n > 0 do
        (t := a; a := b; b := t + b; n := n - 1);
      a
    end
in
  for i := 1 to 10 do
    print(fib(20))
end
