{
    "env": {
        "browser": true,
        "commonjs": true,
        "jest": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "object-property-newline": "off",
        "func-names": "off",
        "no-console": "off"
    }
}